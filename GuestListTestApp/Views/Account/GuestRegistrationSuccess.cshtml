@using GuestListTestApp.Models
@model GuestViewModel
@{
    ViewBag.Title = Resources.Resources.ThankyouMessage;
}

<div class="panel panel-default">
    <div class="panel-heading">
        @Resources.Resources.ThankyouMessage
    </div>
    <div class="panel-body">
        <h5 class="card-title">@Resources.Resources.SuccesRegistrationMessage</h5>
        <p class="card-text">@Resources.Resources.ThankyouForRegistrationMessage</p>
        <a href="#" class="btn btn-primary">Go somewhere</a>
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}